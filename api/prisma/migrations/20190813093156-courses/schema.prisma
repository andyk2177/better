datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/better"
}

generator photonjs {
  provider = "photonjs"
}

generator nexus_prisma {
  provider = "nexus-prisma"
}

model User {
  id        String   @id @default(cuid())
  name      String
  email     String   @unique
  courses   Course[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Course {
  id        String   @id @default(cuid())
  name      String
  title     String?
  users     User[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}